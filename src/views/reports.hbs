        <div class="container reports-container">
            <div class="page-header">
                <div class="container">
                   <div class="row">
                      <div class="col-md-12">
                         <h1>Reportes</h1>
                      </div>
                   </div>
                </div>
             </div>

            <div class="kpi-section row">
                <div class="kpi-container col-sm-3">
                    <div class="kpi">
                        <div class="kpi-header"><h4>Reuniones Creadas</h4></div>
                        <span class="kpi-value">{{meetingsNum}}</span>
                    </div>
                </div>
                <div class="kpi-container col-sm-3">
                    <div class="kpi">
                        <div class="kpi-header"><h4 class="kpi-header">GPA Promedio</h4></div>
                        <span class="kpi-value">{{gpa}}</span>
                    </div>
                </div>
                <div class="kpi-container col-sm-3">
                    <div class="kpi">
                        <div class="kpi-header"><h4 class="kpi-header">Usuarios Activos</h4></div>
                        <span class="kpi-value">{{userNum}}</span>
                    </div>
                </div>
                <div class="kpi-container col-sm-3">
                    <div class="kpi">
                        <div class="kpi-header"><h4 class="kpi-header">Estudiantes Condicionados</h4></div>
                        <span class="kpi-value">{{conditionedNum}}</span>
                    </div>
                </div>
            </div>
            <div class="section-header header-left">
                <div class="container">
                   <div class="row">
                      <div class="col-md-12">
                         <h2>Historial de Reuniones</h2>
                      </div>
                   </div>
                </div>
             </div>
            <div class="meetings-table-container">
                <table id="meetingsTable"class="table table-hover meetings-table">
                    <thead class="thead-dark">
                        <tr>
                            {{#if isProfessor}}
                                <th scope="col">Número</th>
                                <th scope="col">Tema</th>
                                <th scope="col">Descripción</th>
                                <th scope="col">Fecha</th>
                                <th scope="col">Profesor</th>
                                <th scope="col">Estudiante</th>
                                <th scope="col">Estado</th>
                                <th></th>
                                <th></th>
                            {{else }}
                                <th scope="col">Número</th>
                                <th scope="col">Tema</th>
                                <th scope="col">Descripción</th>
                                <th scope="col">Fecha</th>
                                <th scope="col">Profesor</th>
                                <th scope="col">Estudiante</th>
                                <th scope="col">Estado</th>
                            {{/if}}
                        </tr>
                    </thead>
                    <tbody>
                        {{#if isProfessor}}
                            {{#each meetings}}
                                <tr>
                                    <td scope="row">{{this.id}}</td>
                                    <td>{{this.tema}}</td>
                                    <td>{{this.descripcion}}</td>
                                    <td>{{this.fecha}}</td>
                                    <td>{{this.nombres_profesor}} {{this.apellidos_profesor}}</td>
                                    <td>{{this.nombres_estudiante}} {{this.apellidos_estudiante}}</td>
                                    <td>{{this.estado}}</td>
                                    <td><a href="/meetings/edit/{{this.id}}">Editar</a></td>
                                    <td><a href="/meetings/delete/{{this.id}}">Eliminar</a></td>
                                </tr>
                            {{/each}}

                        {{else}}
                            {{#each meetings}}
                                <tr>
                                    <td scope="row">{{this.id}}</td>
                                    <td>{{this.tema}}</td>
                                    <td>{{this.descripcion}}</td>
                                    <td>{{this.fecha}}</td>
                                    <td>{{this.nombres_profesor}} {{this.apellidos_profesor}}</td>
                                    <td>{{this.nombres_estudiante}} {{this.apellidos_estudiante}}</td>
                                    <td>{{this.estado}}</td>
                                </tr>
                            {{/each}}

                        {{/if}}
                    </tbody>
                </table>
            </div>
        
        </div>